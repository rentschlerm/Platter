<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="#F4DDDD">

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btn_Diner"
        android:layout_width="209dp"
        android:layout_height="82dp"
        android:text="@string/btn_diner"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.423"
        android:backgroundTint="@color/white"
        android:textColor="#FD4B13"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btn_Admin"
        android:layout_width="213dp"
        android:layout_height="81dp"
        android:text="@string/btn_admin"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_Diner"
        app:layout_constraintVertical_bias="0.116"
        android:backgroundTint="@color/white"
        android:textColor="#FD4B13"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btn_Proceed"
        android:layout_width="170dp"
        android:layout_height="86dp"
        android:text="@string/btn_dinerSubmit"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_Admin"
        app:layout_constraintVertical_bias="0.500"
        android:backgroundTint="@color/white"
        android:textColor="#FD4B13"/>

    <FrameLayout
        android:id="@+id/frame"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"/>

</androidx.constraintlayout.widget.ConstraintLayout>